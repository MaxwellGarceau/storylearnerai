# LLM Service Configuration
# Default configuration for Gemini (recommended for auto-translation)
VITE_LLM_PROVIDER=gemini
VITE_LLM_API_KEY=your-gemini-api-key-here
VITE_LLM_ENDPOINT=https://generativelanguage.googleapis.com/v1beta
VITE_LLM_MODEL=gemini-2.5-flash-lite
VITE_LLM_MAX_TOKENS=2000
VITE_LLM_TEMPERATURE=0.7
VITE_GEMINI_PROJECT_ID=your-project-id # Optional

# Translation Service Configuration
VITE_ENABLE_MOCK_TRANSLATION=true

# Dictionary Service Configuration (Lexicala API)
VITE_DICTIONARY_API_ENDPOINT=https://lexicala1.p.rapidapi.com
VITE_DICTIONARY_API_KEY=your-rapidapi-key-here
VITE_DISABLE_DICTIONARY=false

# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key
VITE_SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Supabase Test Configuration (for E2E integration tests)
VITE_SUPABASE_TEST_URL=http://127.0.0.1:54331
VITE_SUPABASE_TEST_ANON_KEY=test-anon-key
VITE_SUPABASE_TEST_SERVICE_ROLE_KEY=test-service-role-key

# Test User Configuration (for development only)
# These are used by the create-test-user.js script
TEST_USER_EMAIL=test@example.com
TEST_USER_PASSWORD=TestPass123!
TEST_USER_USERNAME=testuser
TEST_USER_DISPLAY_NAME=Test User

# Logging Configuration
LOG_ENABLED=true
LOG_LEVEL=info
LOG_CONSOLE=true
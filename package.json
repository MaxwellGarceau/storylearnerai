{
  "name": "storylearnerai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "test": "vitest --exclude src/__tests__/e2e/**",
    "test:once": "vitest run --exclude src/__tests__/e2e/**",
    "test:coverage": "vitest run --coverage --exclude src/__tests__/e2e/**",
    "test:e2e": "vitest run --run src/__tests__/e2e/",
    "test:all": "vitest run",
    "test:db:start": "./scripts/test-db.sh start",
    "test:db:stop": "./scripts/test-db.sh stop",
    "test:db:reset": "./scripts/test-db.sh reset",
    "test:db:status": "./scripts/test-db.sh status",
    "test:db:run": "./scripts/test-db.sh test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "ci-check": "./scripts/ci-check.sh"
  },
  "dependencies": {
    "@google/genai": "^1.10.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.0",
    "@supabase/supabase-js": "^2.52.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.0",
    "lucide-react": "^0.454.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "jsdom": "^25.0.1",
    "msw": "^2.10.4",
    "postcss": "^8.4.47",
    "serve": "^14.2.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.0.1",
    "vite": "^5.4.19",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "esbuild": "^0.25.0",
    "on-headers": "^1.1.0"
  }
}
